  <title>Home Page</title>
  <style>
    body {
      background-color: white;
      font-family: 'Roboto', sans-serif;
    }
    {{!-- nav --}}
      .nav-link {
      color: white;
      margin: 10px;
      font-size: 16px;
    }
    button .nav-link {
      height: 40px;
      margin: 0 auto;
    }
    {{!-- jumbotron --}}
     .jumbotron {
      background-color: #832161;
      color: white;
      text-align: center;
      font-family: 'Righteous', cursive;
    }
    h1 {
      font-size: 60px;
    }
    {{!-- cards --}}
    .card {
      margin: 20px;
      border-bottom: 2px solid darkslategray;
      border-right: 2px solid darkslategray;
    }
     .card-header  {
      background-color: #DA4167;
    }
     .card-header:hover {
       background-color: #832161;
    }
   h2 {
     color: white;
   }
   h3 {
     color: white;
   }
    a:hover {
       text-decoration: none;
    }
    {{!-- buttons --}}
     button {
      background-color: #832161;
      color: white;
    }
    {{!-- modal --}}
      .note-text  {
      width: 80%;
      height: 60%;
      margin-left: 20px;
    }
    .delete {
      margin: 20px;
    }
    
